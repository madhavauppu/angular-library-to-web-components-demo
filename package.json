{
  "name": "angular-library-to-web-components-demo",
  "version": "1.0.0",
  "author": "Madhava U",
  "keywords": [
    "angular",
    "angular-elements",
    "web-components"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --aot --prod && npm run build:elements",
    "build:elements": "cd projects/elements && sh buildElements.sh && npm run copy-angular-elements && npm run copy-angular-elements-index && npm run copy-angular-elements-js",
    "copy-angular-elements": "cpx \"projects/elements/dist/components/*.*\" \"dist/angular-library-to-web-components-demo/library\"",
    "copy-angular-elements-index": "cpx \"projects/elements/src/index.*\" \"dist/angular-library-to-web-components-demo/library\"",
    "copy-angular-elements-js": "cpx \"projects/elements/src/*.js\" \"dist/angular-library-to-web-components-demo/library\"",
    "serve-prod-build": "npm run compree-dist && http-server -g:true ./dist/angular-library-to-web-components-demo -p 5007 --cors",
    "compree-dist": "gulp compress",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "terser": "^3.14.1",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.2.0",
    "@angular-devkit/build-angular": "~0.901.15",
    "@angular-devkit/build-ng-packagr": "~0.901.15",
    "@angular/animations": "~9.1.13",
    "@angular/cdk": "^9.2.4",
    "@angular/cli": "~9.1.15",
    "@angular/common": "~9.1.13",
    "@angular/compiler": "~9.1.13",
    "@angular/compiler-cli": "~9.1.13",
    "@angular/core": "~9.1.13",
    "@angular/elements": "^9.1.13",
    "@angular/forms": "~9.1.13",
    "@angular/language-service": "~9.1.13",
    "@angular/platform-browser": "~9.1.13",
    "@angular/platform-browser-dynamic": "~9.1.13",
    "@angular/router": "~9.1.13",
    "@carbon/charts": "0.41.79",
    "@carbon/charts-angular": "0.41.79",
    "@carbon/icons-angular": "11.1.4",
    "@carbon/themes": "^10.54.0",
    "@cloudMatrix-CAM/cb-common-ui": "^10.4.0",
    "@cloudMatrix-CAM/ecam-common-ui": "4.6.66",
    "@cloudMatrix-CAM/theme-lib": "^2.9.0",
    "@types/node": "^12.11.1",
    "@webcomponents/custom-elements": "^1.5.0",
    "@webcomponents/webcomponentsjs": "^2.7.0",
    "carbon-addons-angular": "^1.4.1",
    "carbon-components": "~10.28.0",
    "carbon-components-angular": "4.56.4",
    "codelyzer": "^5.1.2",
    "cpx": "^1.5.0",
    "d3": "^5.14.2",
    "d3-color": "1.3.0",
    "document-register-element": "1.8.1",
    "gulp": "^4.0.2",
    "gulp-gzip": "^1.4.2",
    "http-server": "^14.1.0",
    "moment-timezone": "^0.5.34",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.0",
    "rxjs": "~6.6.7",
    "rxjs-compat": "^6.5.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.8.3",
